  }

  dimension: county_number {
    view_label: "Store"
    label: "County Fips"
    type: string
    sql: CONCAT('190',${TABLE}.county_number) ;;
    map_layer_name: us_counties_fips
  }

  dimension: category {
    view_label: "Item"
    type: string
    sql: ${TABLE}.category ;;
  }

  dimension: vendor_number {
    view_label: "Vendor"
    type: string
    sql: ${TABLE}.vendor_number ;;
  }

  dimension: item_number {
    view_label: "Item"
    type: string
    sql: ${TABLE}.item_number ;;
  }

  dimension_group: calendar {
    datatype: date
    type: time
    sql: ${TABLE}.date ;;
  }

  dimension: pack {
    view_label: "Item"
    hidden: yes
    type: number
